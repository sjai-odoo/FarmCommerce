<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="inherited_product_form_view" model="ir.ui.view">
        <field name="name">inherited.product.property.view.form</field>
        <field name="model">product.property</field>
        <field name="inherit_id" ref="Farming.product_property_view_form"></field>
        <field name="arch" type="xml">
            <xpath expr="//sheet" position="inside">
                <div class="oe_button_box" name="button_box">
                    <button name="%(action_farmers_details)d" class="oe_stat_button" icon="fa-file-image-o" string="Farmers" type="action" />
                </div>
            </xpath>
        </field>
    </record>
</odoo>
